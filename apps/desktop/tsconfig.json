{
  "extends": "@umami/typescript-config/tsconfig.json",
  "include": ["src", "vite.config.ts", ".eslintrc.js", "jest.config.ts"],
  "typedocOptions": {
    "entryPoints": ["src"],
    "entryPointStrategy": "expand",
    "excludeNotDocumented": false,
    "out": "docs",
    "plugin": ["typedoc-plugin-missing-exports"]
  }
}
