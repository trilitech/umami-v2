{
  "default": {
    "backtrace": true,
    "retry": 1,
    "parallel": 1,
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "dryRun": false,
    "failFast": false,
    "paths": ["src/e2e/features/"],
    "require": ["src/e2e/steps/*.ts"],
    "requireModule": ["ts-node/register"],
    "format": ["html:test-results/cucumber-report.html", "json:test-results/cucumber-report.json"]
  }
}
