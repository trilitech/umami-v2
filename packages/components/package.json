{
  "name": "@umami/components",
  "packageManager": "pnpm@9.9.0",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "@umami/source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/babel__core": "^7.20.5",
    "@types/eslint": "^8",
    "@types/lodash": "^4",
    "@types/md5": "^2.3.5",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/zxcvbn": "^4.4.5",
    "@umami/eslint-config": "workspace:^",
    "@umami/jest-config": "workspace:^",
    "@umami/multisig": "workspace:^",
    "@umami/state": "workspace:^",
    "@umami/test-utils": "workspace:^",
    "@umami/typescript-config": "workspace:^",
    "babel-jest": "^29.7.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "madge": "^8.0.0",
    "prettier": "^3.3.3",
    "react-redux": "^9.1.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "tsup-node --dts",
    "build:quick": "tsup-node",
    "clean": "rimraf build dist .turbo",
    "check-circular-deps": "madge --circular src/index.ts",
    "check-types:watch": "pnpm check-types --watch",
    "check-types": "tsc",
    "dev": "tsup-node --watch",
    "format:ci": "prettier --ignore-path ../../.gitignore --check .",
    "format": "prettier --ignore-path ../../.gitignore --write .",
    "lint:ci": "eslint src --ext .ts --max-warnings=0",
    "lint": "eslint src --ext .ts --fix",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^3.1.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@umami/core": "workspace:^",
    "@umami/tezos": "workspace:^",
    "@umami/tzkt": "workspace:^",
    "bip39": "^3.1.0",
    "framer-motion": "^11.11.17",
    "md5": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-remove-scroll": "^2.6.0",
    "zod": "^3.23.8",
    "zxcvbn": "^4.4.2"
  }
}
