{
  "name": "@umami/mobile-components",
  "packageManager": "pnpm@9.9.0",
  "version": "1.0.0",
  "description": "A reusable components library for Expo and React Native apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "test": "jest",
    "clean": "rm -rf dist",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.76.3",
    "expo-linear-gradient": "~12.0.1",
    "react-native-svg": "13.12.0"
  },
  "peerDependencies": {
    "expo": "*",
    "react": ">=18.0.0",
    "react-native": ">=0.71.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/vector-icons": "^14.0.2",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^6.7.1",
    "@typescript-eslint/parser": "^6.7.1",
    "@umami/eslint-config": "workspace:^",
    "@umami/typescript-config": "workspace:^",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.4",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "expo",
    "react-native",
    "components",
    "library",
    "ui"
  ],
  "private": true
}
