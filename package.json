{
  "name": "umami",
  "productName": "umami",
  "version": "2.0.0",
  "private": true,
  "author": "Trilitech <umami-admin@trili.tech>",
  "description": "Tezos Wallet",
  "homepage": "./",
  "main": "./public/electron.js",
  "dependencies": {
    "@airgap/tezos": "^0.13.11",
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@ledgerhq/hw-transport-webhid": "^6.27.13",
    "@reduxjs/toolkit": "^1.9.3",
    "@taquito/ledger-signer": "^16.1.1",
    "@taquito/signer": "^15.1.0",
    "@taquito/taquito": "^15.1.0",
    "@taquito/utils": "^15.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@toruslabs/customauth": "^11.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.14",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@tzkt/oazapfts": "^4.5.2",
    "@tzkt/sdk-api": "^2.2.0",
    "axios": "^1.3.4",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^2.29.3",
    "ed25519-hd-key": "^1.3.0",
    "framer-motion": "^10.2.5",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "mockdate": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-icons": "^4.8.0",
    "react-identicons": "^1.2.5",
    "react-native-safe-area-context": "3.3.2",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "sort-by": "^1.2.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "zod": "^3.21.4"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "cross-env TZ=CET react-scripts test --coverage --verbose --testPathIgnorePatterns='src/integration/*'",
    "test:tezos:integration": "cross-env TZ=CET react-scripts test --watchPathIgnorePatterns=[]",
    "typecheck": "tsc",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "electron:start": "concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "build-storybook": "build-storybook -s public",
    "electron:package:mac": "electron-builder -m -c.extraMetadata.main=build/electron.js -c.mac.identity=null",
    "electron:package:win": "electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/assert": "^1.5.6",
    "@types/prop-types": "^15.7.5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/testing-library__user-event": "^4.2.0",
    "@types/wait-on": "^5.3.1",
    "assert": "^2.0.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.38.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "react-app-rewired": "^2.2.1",
    "stream-http": "^3.2.0",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0"
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs",
      "@ledgerhq/devices/hid-framing": "@ledgerhq/devices/lib/hid-framing"
    },
    "watchPathIgnorePatterns": [
      "src/integration"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 45,
        "functions": 40,
        "lines": 50,
        "statements": 50
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "build": {
    "appId": "com.electron.myapp",
    "productName": "Umami",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "icon": "./public/logo512.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64",
            "x64"
          ]
        }
      ]
    },
    "win": {
      "icon": "build/logo512.png",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": "AppImage",
      "mimeTypes": [
        "x-scheme-handler/umami"
      ]
    },
    "protocols": {
      "name": "umami",
      "schemes": [
        "umami"
      ]
    }
  }
}
